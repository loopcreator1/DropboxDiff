{
	"name": "DropboxDiff",
	"version": "1.0.1",

	"background_page": "background.html",
	"content_scripts": [
		{
			"css": [
				"styles.css"
			],
			"js": [
				"jquery-1.6.2.min.js",
				"content.js"
			],
			"matches": [ "https://www.dropbox.com/revisions/*" ],
			"run_at": "document_end"
		}
	],
	"icons": {
		"128": "128.png",
		"16": "16.png",
		"48": "48.png"
	},
	"options_page": "options.html",
	"permissions": [
		"https://*.dropbox.com/*",
		"tabs"
	],
	"plugins": [
		{ "path": "plugin/npDropboxDiffPlugin.dll" },
		{ "path": "plugin/DropboxDiffPlugin.plugin" },
		{ "path": "plugin/npDropboxDiffPlugin.so" }
	]
}
